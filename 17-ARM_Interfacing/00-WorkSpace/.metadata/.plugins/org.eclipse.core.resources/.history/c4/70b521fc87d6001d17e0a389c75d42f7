#ifndef USART_PRIVATE_H_
#define USART_PRIVATE_H_

#include "Std_Types.h"

// All on APB1 Bus
#define				USART_6				((volatile u32*)0x40011400)
#define				USART_1				((volatile u32*)0x40011000)
#define				USART_2				((volatile u32*)0x40004400)


#endif /* USART_PRIVATE_H_ */
